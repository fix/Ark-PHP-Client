{
    "name": "faustbrian/ark-php-client",
    "description": "An Ark Client for PHP",
    "keywords": ["api", "path of exile", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "faustbrian/http": "^1.0@dev",
        "stephenhill/base58": "^1.1"
    },
    "require-dev": {
        "coderabbi/phpumoji": "^0.7.0@beta",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.2",
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Ark\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Ark\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
